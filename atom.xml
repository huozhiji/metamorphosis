layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Metamorphosis</title>
 <link href="http://metaphox.name/metamorphosis/feed/atom.xml" rel="self" />
 <link href="http://metaphox.name/metamorphosis/feed/" />
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://metaphox.name/metamorphosis/</id>
 <author>
   <name>Metaphox</name>
   <email>foxisme@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title | xml_escape }}</title>
   <link href="http://metaphox.name/metamorphosis{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://metaphox.name/metamorphosis{{ post.url }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
